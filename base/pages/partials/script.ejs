window.onload=function(){
Vue.component('filter-data', {
  template: ``,
  props: ['title']
})
new Vue({
  el: '#container',
  data: {
    newTodoText: '',
    search:"",
    todos: [
    	'Take out the trash',
      'Do the dishes',
      'Mow the lawn'
    ]
  },
  methods: {
    abcd:function(){
    	this.todosSmart.sort(function(a,b){
      	return a > b;
      });
    }
  },
  computed:{
  	todosSmart:function(){
    	if(this.search === "" ){
      	return this.todos;
      }else{
      	var search = this.search;
      	return this.todos.filter(function(todo){
        var reg = new RegExp(search, 'g');
    			return reg.test(todo);
    });
      }

    }
  }
})
